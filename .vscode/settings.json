	"files.exclude": {
		"**/.github/cicd/": true,
		"**/*.vs": true,
		"**/*.idea": true,
		"**/*.run": true,
		"**/bin": true,
		"**/*.sln.DotSettings*": true,
		"**/*.code-workspace*": true,
		"*.lock": true,
	},
	"dotnet.defaultSolution": "Velaptor.sln",
	"[csharp]": {
		"editor.insertSpaces": false,
		"editor.indentSize": 4,
	},
	"[jsonc]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
	},
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
	},
	"editor.detectIndentation": false,
	"[github-actions-workflow]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	}
}
